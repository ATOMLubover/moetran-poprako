{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "moetran-native",
  "version": "0.1.0",
  "identifier": "com.moetran-native.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:5174",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "moetran-poprako",
        "width": 1440,
        "height": 720
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/poprako-tmp.ico"]
  }
}
